{
    "title":"sort",
    "desc":"sort a matrix/map based on axis and given columns ",
    "input":[
	        {
                "str":"data", 
                "type":["array", "map"], 
                "description":"map key is column name, map value are column values.", 
                "optional":false
            }
	    ],
    "params":[
        {
            "str":"ascending", 
            "type":"bool", 
            "description":"Sorting data in ascending", 
            "default" : false,
            "optional":true
        },
        {
            "str":"axis", 
            "type":"int", 
            "description":"0=vertical/column, 1=horizontal/row", 
            "default" : 0,
            "allowed" : [0, 1],
            "optional":true
        },
        {
            "str":"nilPosition", 
            "type":"string", 
            "description":"Puts Nils at the beginning if first; last puts Nils at the end.", 
            "allowed" : ["first", "last"],
            "optional":true
        },
        {
            "str":"by", 
            "type":"array", 
            "description":"sort by elements: 1. all are string -> conlumn label, 2. all are int (0 means first column) -> column number", 
            "optional":false
        }
    ],
    "output":["array", "map"]
}